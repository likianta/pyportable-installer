{
    "app_name": "Project Example 03",
    "app_version": "0.2.2",
    "description": "Test packaging with pip.",
    "authors": [],
    "build": {
        "proj_dir": ".",
        "dist_dir": "../../dist/{app_name_kebab}-{app_version}",
        "launchers": {
            "{app_name}": {
                "file": "main.py",
                "icon": "",
                "function": "",
                "args": [],
                "kwargs": {}
            }
        },
        "readme": "",
        "attachments": {},
        "attachments_exclusions": [],
        "attachments_exist_scheme": "override",
        "module_paths": [],
        "module_paths_scheme": "translate",
        "python_version": "3.10",
        "venv": {
            "enable_venv": true,
            "mode": "pip",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_lower}_venv",
                    "venv_id": "",
                    "requirements": [],
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": "",
                    "copy_venv": true
                },
                "pip": {
                    "requirements": "./requirements.txt",
                    "pypi_url": "https://pypi.python.org/simple/",
                    "offline": false,
                    "local": ""
                },
                "embed_python": {
                    "path": ""
                }
            }
        },
        "compiler": {
            "name": "pyportable_crypto",
            "options": {
                "cythonize": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "pyarmor": {
                    "license": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "{random}"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "experimental_features": {
            "add_pywin32_support": false,
            "platform": "system_default"
        },
        "enable_console": true
    },
    "note": [
        "To have a full test schedule, firstly delete '<python>/lib/site-packages/embed-python-manager/assets' folder (if exists).",
        "For Windows users, you must run this application in Windows Terminal, not CMD.exe. Because the latter doesnot support lk-logger logging colors."
    ],
    "pyportable_installer_version": "4.3.1"
}
