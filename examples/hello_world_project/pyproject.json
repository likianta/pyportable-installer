{
    "app_name": "Hello World",
    "app_version": "0.5.4",
    "description": "Say hello to the given name.",
    "authors": [
        "Likianta <likianta@foxmail.com>"
    ],
    "build": {
        "proj_dir": "./hello_world",
        "dist_dir": "../../dist/{app_name_lower}_{app_version}",
        "launcher_name": "{app_name}",
        "icon": "",
        "target": [
            {
                "file": "hello_world/hello.py",
                "function": "main",
                "args": [],
                "kwargs": {}
            }
        ],
        "readme": "",
        "attachments": {},
        "attachments_exclusions": [],
        "attachments_exist_scheme": "override",
        "module_paths": [],
        "module_paths_scheme": "translate",
        "venv": {
            "enable_venv": true,
            "python_version": "3.10",
            "mode": "depsland",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_lower}_venv",
                    "venv_id": "",
                    "requirements": "",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": ""
                },
                "pip": {
                    "requirements": "",
                    "pypi_url": "https://pypi.python.org/simple/",
                    "local": "",
                    "offline": false
                },
                "embed_python": {
                    "path": ""
                }
            }
        },
        "compiler": {
            "name": "pyportable_crypto",
            "options": {
                "cython": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "mypyc": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "nuitka": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "_trial",
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "experimental_features": {
            "add_pywin32_support": false
        },
        "enable_console": true
    },
    "note": "",
    "pyportable_installer_version": "4.0.0"
}
