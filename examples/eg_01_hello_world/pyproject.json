{
    "app_name": "Hello World",
    "app_version": "0.9.4",
    "description": "Say hello to the given name.",
    "authors": [
        "Likianta <likianta@foxmail.com>"
    ],
    "build": {
        "proj_dir": "./hello_world",
        "dist_dir": "../../dist/{app_name_kebab}-{app_version}",
        "launchers": {
            "{app_name_kebab}": {
                "file": "hello_world/hello.py",
                "icon": "../eg_02_pyportable_installer_bootstrap/launcher.ico",
                "function": "main",
                "args": [],
                "kwargs": {}
            }
        },
        "readme": "",
        "attachments": {},
        "attachments_exclusions": [],
        "attachments_exist_scheme": "overwrite",
        "module_paths": [],
        "module_paths_scheme": "translate",
        "python_version": "3.8",
        "venv": {
            "enabled": false,
            "mode": "depsland",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_snake}_venv",
                    "venv_id": "",
                    "requirements": "",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": ""
                },
                "pip": {
                    "requirements": "",
                    "pypi_url": "https://pypi.python.org/simple/",
                    "local": "",
                    "offline": false
                },
                "embed_python": {
                    "path": ""
                }
            }
        },
        "compiler": {
            "enabled": true,
            "mode": "pyportable_crypto",
            "options": {
                "cython": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "mypyc": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "nuitka": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0,
                    "python_path": "auto_detect"
                },
                "pyportable_crypto": {
                    "key": "{random}"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "experimental_features": {
            "add_pywin32_support": false,
            "platform": "windows"
        },
        "enable_console": true
    },
    "note": "",
    "pyportable_installer_version": "4.4.0"
}
