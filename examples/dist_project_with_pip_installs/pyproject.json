{
    "app_name": "Test Project",
    "app_version": "0.1.4",
    "description": "",
    "authors": [],
    "build": {
        "proj_dir": ".",
        "dist_dir": "../../dist/{app_name_lower}_{app_version}",
        "launcher_name": "{app_name}",
        "icon": "",
        "target": [
            {
                "file": "./main.py",
                "function": "",
                "args": [],
                "kwargs": {}
            }
        ],
        "readme": "",
        "attachments": {},
        "attachments_exist_scheme": "override",
        "module_paths": [],
        "venv": {
            "enable_venv": true,
            "python_version": "3.9",
            "mode": "pip",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_lower}_venv",
                    "venv_id": "",
                    "requirements": "",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": ""
                },
                "pip": {
                    "requirements": "./requirements.txt",
                    "pypi_url": "https://pypi.python.org/simple/",
                    "offline": false,
                    "local": ""
                },
                "embed_python": {
                    "path": ""
                }
            }
        },
        "compiler": {
            "name": "pyc",
            "options": {
                "cythonize": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "{random}",
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "enable_console": true
    },
    "note": "",
    "pyportable_installer_version": "4.0.0a1"
