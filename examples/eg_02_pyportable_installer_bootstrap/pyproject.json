{
    "app_name": "PyPortable Installer",
    "app_version": "4.4.0-build.11",
    "description": "",
    "authors": [
        "Likianta <likianta@foxmail.com>"
    ],
    "build": {
        "proj_dir": "../../pyportable_installer",
        "dist_dir": "../../dist/{app_name_kebab}-{app_version}",
        "launchers": {
            "{app_name}": {
                "file": "../../pyportable_installer/user_interface/gui_on_psg.py",
                "icon": "launcher.ico",
                "function": "main",
                "args": [],
                "kwargs": {}
            }
        },
        "readme": "",
        "attachments": {
            "../../dist": "only_folder",
            "../../temp": "only_folder"
        },
        "attachments_exclusions": [],
        "attachments_exist_scheme": "overwrite",
        "module_paths": [],
        "module_paths_scheme": "translate",
        "python_version": "3.8",
        "venv": {
            "enabled": false,
            "mode": "depsland",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_snake}_venv",
                    "venv_id": "",
                    "requirements": "../../requirements.txt",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": ""
                },
                "pip": {
                    "requirements": [],
                    "pypi_url": "https://pypi.python.org/simple/",
                    "local": "",
                    "offline": false
                },
                "embed_python": {
                    "path": ""
                }
            }
        },
        "compiler": {
            "enabled": true,
            "mode": "pyportable_crypto",
            "options": {
                "cython": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "mypyc": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "nuitka": {
                    "c_compiler": "msvc",
                    "python_path": "auto_detect"
                },
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "{random}"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "experimental_features": {
            "add_pywin32_support": false,
            "platform": "system_default"
        },
        "enable_console": false
    },
    "note": "",
    "pyportable_installer_version": "4.4.0"
}
